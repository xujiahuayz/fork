from fork_env.integrationCy import integrate_function

a = 0.0
b = 2.0
%timeit integrate_function(a, b)

from scipy.integrate import quad
# from libc.math cimport pow

def integrate_function_py(a, b):
    """
    Integrates the function f(x) = x^2 from a to b using scipy's quad function.
    """
 

    def integrand(x):
        return x**2

    result, error = quad(integrand, a, b)
    return result

%timeit integrate_function_py(a, b)
